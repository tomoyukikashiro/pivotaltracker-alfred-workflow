				<string>import sys
import os
import urllib2
import json

token = os.environ['token']
api_url = os.environ['api_url']
project_id = os.environ['project_id']
scope = os.environ['scope']


headers = {'X-TrackerToken' : token, 'X-Tracker-Pagination-Limit': 300}

if scope == 'all':
	request = urllib2.Request('{}/projects/{}/stories'.format(api_url, project_id), headers=headers)
	stories = json.loads(urllib2.urlopen(request).read())
else:
	request = urllib2.Request('{}/projects/{}/iterations?scope={}'.format(api_url, project_id, scope), headers=headers)
	stories = json.loads(urllib2.urlopen(request).read())[0].get('stories', [])

def make_item(s):
	return {
		'title': s['name'],
		'subtitle': 'STATUS: {}  /  LABLES: {}'.format(s['current_state'], ', '.join([ l['name'] for l in s['labels']])),
		'arg': s['id'],
		'icon': {'type': 'default', 'path': '{}.png'.format(s['story_type'])},
		'autocomplete': '{} {}'.format(s['name'], s['story_type']),
		'text': {'copy': s['url']},
		'quicklookurl': s['url']
	}

formatted_stories = [ make_item(s) for s in stories ]

sys.stdout.write(json.dumps({'items': formatted_stories}))
