				<string>import sys
import os
import urllib2
import json

token = os.environ['token']
api_url = os.environ['api_url']
base_url = os.environ['base_url']

request = urllib2.Request("{}/projects".format(api_url), headers={"X-TrackerToken" : token})
projects = json.loads(urllib2.urlopen(request).read())

def make_item(p):
	url = '{}/{}'.format(base_url, p['id'])
	return {
		'title': p['name'],
		'subtitle': p.get('description', ''),
		'arg': p['id'],
		'icon': {}, 
		'autocomplete': '{}/{}'.format(p['name'], p.get('description', '')),
		'text': {'copy': url},
		'quicklookurl': url
	}

formatted_projects = [ make_item(p) for p in projects ]

sys.stdout.write(json.dumps({'items': formatted_projects}))</string>
